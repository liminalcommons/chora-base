# =============================================================================
# SAP-Specific Ignores
# =============================================================================

{% if ((sap_selection_mode in ['standard', 'comprehensive']) or (sap_selection_mode == 'custom' and (include_sap_010 | default(false)))) %}# SAP-010: Memory System
# Archive old event logs (keep current month only)
.chora/memory/events/archive/

# Temporary memory files
.chora/memory/.tmp/
{% endif %}

{% if (sap_selection_mode != 'custom' or (include_sap_015 | default(false))) %}# SAP-015: Beads
# Beads daemon socket (if using daemon mode)
.beads/.daemon.sock

# Beads temporary files
.beads/.tmp/
{% endif %}

{% if ((sap_selection_mode in ['standard', 'comprehensive']) or (sap_selection_mode == 'custom' and (include_sap_053 | default(false)))) %}# SAP-053: Conflict Resolution
# Conflict detection cache (optional, can be regenerated)
.conflict-cache/
{% endif %}

{% if ((sap_selection_mode == 'comprehensive') or (sap_selection_mode == 'custom' and (include_sap_056 | default(false)))) %}# SAP-056: Lifecycle Traceability
# Generated feature manifest cache
.feature-manifest-cache.json
{% endif %}

